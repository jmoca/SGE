                       </columns>
                     </TableView>
                  </children></AnchorPane>
           </content>
         </Tab>
         <Tab text="Ventas">
           <content>
             <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                  <children>
                     <TableView prefHeight="223.0" prefWidth="426.0">
                       <columns>
                         <TableColumn prefWidth="75.0" text="C1" />
                         <TableColumn prefWidth="75.0" text="C2" />
                       </columns>
                     </TableView>
                  </children></AnchorPane>
           </content>
         </Tab>
     </tabs>
   </TabPane>
   <Button fx:id="datosProductos" layoutX="40.0" layoutY="14.0" mnemonicParsing="false" onAction="#onHelloButtonClick" text="DatosInventario" />
</AnchorPane>
