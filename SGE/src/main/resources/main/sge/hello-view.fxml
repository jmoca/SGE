                           <TableColumn fx:id="precioProducto" prefWidth="126.0" text="Precio" />
                       </columns>
                     </TableView>
                  </children>
               </AnchorPane>
         </content>
       </Tab>
       <Tab text="Clientes">
         <content>
           <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="171.0" prefWidth="377.0">
                  <children>
                     <TableView layoutX="6.0" prefHeight="226.0" prefWidth="345.0">
                       <columns>
                         <TableColumn prefWidth="94.0" text="nombre" />
                           <TableColumn prefWidth="107.0" text="dirección" />
                           <TableColumn prefWidth="143.0" text="contacto" />
                       </columns>
                     </TableView>
                  </children></AnchorPane>
         </content>
       </Tab>
         <Tab text="Proveedores">
           <content>
             <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                  <children>
                     <TableView prefHeight="230.0" prefWidth="426.0">
                       <columns>
                         <TableColumn fx:id="nombreProveedores" prefWidth="75.0" text="nombre" />
                         <TableColumn fx:id="direccionProveedores" prefWidth="75.0" text="dirección" />
                           <TableColumn fx:id="contactoProveedores" prefWidth="75.0" text="contacto" />
