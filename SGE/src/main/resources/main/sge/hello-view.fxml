       <Tab text="Productos">
         <content>
           <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                  <children>
                     <TableView fx:id="tableView" layoutX="-1.0" layoutY="14.0" prefHeight="200.0" prefWidth="433.0">
                       <columns>
                         <TableColumn fx:id="nombreProducto" prefWidth="151.0" text="Nombre" />
